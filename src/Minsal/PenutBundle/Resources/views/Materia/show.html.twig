{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Materia</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Codigomateria</th>
                <td>{{ entity.codigoMateria }}</td>
            </tr>
            <tr>
                <th>Nombremateria</th>
                <td>{{ entity.nombreMateria }}</td>
            </tr>
            <tr>
                <th>Unidadesvalorativas</th>
                <td>{{ entity.unidadesValorativas }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('materia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('materia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
